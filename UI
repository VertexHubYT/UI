		for i,v in next, Windows do
			v.Size = UDim2.new(0, 187, 0, v.Header.ObjectFrame.UIListLayout1.AbsoluteContentSize.Y + 45)
			v.Header.ObjectFrame.Size = UDim2.new(0, 173, 0, v.Size.Y.Offset - 45)
        end
